{
  "name": "vue-test-component",
  "version": "1.0.0",
  "description": "This is a demo for lerna-test",
  "author": "xufanglin",
  "license": "MIT",
  "main": "",
  "scripts": {
    "test": "jest",
    "coverage": "open ./coverage/lcov-report/index.html"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.33",
    "babel-core": "^6.26.3",
    "babel-jest": "^25.4.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^25.3.0",
    "jest-serializer-vue": "^2.0.2",
    "lerna": "^3.20.2",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.11"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "babel-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**",
      "!**/coverage/**"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ]
  }
}
